<div class="panel panel-default">
	<div class="panel-heading">
		<h3 class="panel-title">Most Popular</h3>
	</div>

	<div class="panel-body">
		{{#each sidebar.popular}}
			<div class="col-sm-4 text-center" style="padding-bottom: .5em;">
				<a href="/images/{{image.uniqueId}}">
					<img src="/public/upload/{{image.filename}}" class="img-thumbnail" style="width: 75px; height: 75px;">
				</a>
			</div>
		{{/each}}
	</div>
</div>